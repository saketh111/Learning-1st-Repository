<apex:page standardController="sales__c" renderAs="pdf">
   <apex:form >
      <apex:pageBlock title="SalesEdit">
          <apex:pageBlocksection title="Sales Information">
             <apex:outputfield value="{!sales__c.Account__c}"/>
             <apex:outputfield value="{!sales__c.Location__c}"/>
             <apex:outputfield value="{!sales__c.Quaters__c}"/>
             <apex:outputfield value="{!sales__c.revenue__c}"/>
           </apex:pageBlockSection>
         
<apex:pageBlockButtons >
<apex:commandButton action="{!save}" value="save"/>
<apex:commandButton action="{!Cancel}" value="Cancel"/>
</apex:pageBlockButtons>
</apex:pageBlock>


</apex:form>
</apex:page>