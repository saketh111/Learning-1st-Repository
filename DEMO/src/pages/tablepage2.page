<apex:page standardController="Contact" recordSetVar="Contacts" >
  <apex:form >
    <apex:pageblock >
        <apex:pageBlockTable value="{!Contacts}" var="a">
            <apex:column value="{!a.Name}"/>
            <apex:column value="{!a.Department}"/>
            <apex:column value="{!a.Phone }"/>
            
       </apex:pageBlockTable>
   </apex:pageblock>
   </apex:form>  
</apex:page>